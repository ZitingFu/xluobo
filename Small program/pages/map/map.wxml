<!--logs.wxml-->
<view class="container log-list">
	<map
	  id="map"
	  latitude="{{la}}"
	  longitude="{{lo}}"
	  scale="14"
	  controls="{{controls}}"
	  bindcontroltap="controltap"
	  markers="{{markers}}"
	  bindmarkertap="markertap"
	  polyline="{{polyline}}"
	  bindregionchange="regionchange"
	  show-location
	  style="width: 100%; height:100%;"
	></map>
</view>
