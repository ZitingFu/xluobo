<!--logs.wxml-->
<view class="container log-list">
	<view class=""></view>
	<view id="meCollection">
   		<view class="Release-center" >
   		 	<view class="nearby clearFix">
				<view class="nearby-center clearFix">
					<view class="nearby-top brbott-tae7e7e7" data-id="{{item.id}}" wx:for="{{questionItem}}" wx:key="" bindtap="details">
						<view class="clearFix genre_name_top">
							<view class="f-left f333 fontSize30 genre_name"><image class="jing" src="{{jing}}" alt=""></image>{{item.genre_name}}</view>
							<text class="f-right f999">{{item.time}}</text>
						</view>
						<view class="clearFix content" style="width: 100%;">{{item.content}}</view>
						<view class="list-imgbox">
							<!-- 图片为6张 -->
							<view class="SixImg clearFix">
								<!-- 图片6张 -->
								<view catchtap="imgtop"
									  class="SixImg-img"
									  wx:if="{{item.images.length > 4}}" 
									  wx:for="{{item.images}}" 
									  data-list="{{item.images}}" 
									  data-index='{{index}}'  
									  wx:key="index" 
									  wx:for-item="iteming">
							    	<image src="{{iteming.s}}" alt=""></image>
							    </view>
							    <!-- 图片2.3张 -->
								<view catchtap="imgtop"
									  class="SixImg-img"
									  wx:if="{{item.images.length ==2 || item.images.length == 3}}" 
									  wx:for="{{item.images}}" 
									  data-list="{{item.images}}" 
									  data-index='{{index}}'  
									  wx:key="index" 
									  wx:for-item="iteming">
							    	<image src="{{iteming.s}}" alt=""></image>
							    </view>
							    <!-- 图片1张 -->
							  	<view catchtap="imgtop"
									  class="SixImg-img"
									  wx:if="{{item.images.length == 1}}" 
									  wx:for="{{item.images}}" 
									  data-list="{{item.images}}" 
									  data-index='{{index}}'  
									  wx:key="index"
									  style="width:80%;height:200px;" 
									  wx:for-item="iteming">
							    	<image src="{{iteming.s}}" alt=""></image>
							    </view>
							    <!-- 图片4张 -->
							    <view style="width:80%" class="clearFix">
								  	<view catchtap="imgtop"
										  class="SixImg-img"
										  wx:if="{{item.images.length == 4}}" 
										  wx:for="{{item.images}}" 
										  data-list="{{item.images}}" 
										  data-index='{{index}}'  
										  wx:key="index"
										  style="width:111px;height:110px;margin-right:5px"
										  wx:for-item="iteming">
								    	<image src="{{iteming.s}}" alt=""></image>
								    </view>
							    </view>
							    <!-- video -->
							    <!-- <video objectFit="fill" wx:if="{{item.video}}" style="margin-top:13px;" src="{{item.video}}"></video> -->
						    </view>
					    	<view class="nearby-bottom paddBo13 paddTa13 clearFix">
			   			 		<view class="clearFix height40">
									<view class="f-left f333 fontSize30 wi70 hidden">
										<view  class="wihe20 f-left paddRi14" style="top:7px;">
										 	<image wx:if="{{item.avatar.s}}" src="{{item.avatar.s}}"></image>
										 	<image wx:else src="{{loge}}"></image>
										</view>
										<view class="f-left hidden wi70" style="line-height: 40px">
											{{item.name}}
										</view>
									</view>
									<view class="f-right wi23 f999 clearFix">
										<view class="wihe20 f-left paddRi9 "><image class="xinxi" src="{{xinxi}}" alt=""></image></view>
										<view class="f-left length">0</view>
									</view>
								</view>
			   			 	</view>
						 </view>
					</view>
				</view>
			</view>
   		</view>
	</view>
</view>
